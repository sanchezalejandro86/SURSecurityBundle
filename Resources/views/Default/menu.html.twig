{% spaceless %}
<div id="cssmenu">
	<ul id="jMenu">
		{% for h in menu %}
			<li class='has-sub'>
				<a href="{{ (h.nodoArchivo!= '#') ? path(h.nodoArchivo) : h.nodoArchivo }}" class="fNiv">{{ h.nodoNombre }}</a>
				{% if h.hijos | default() %}
					<ul>
						{{ _self.r(h.hijos)}}
						{% macro r(m) %}
						    {% for s in m %}
						        <li class='has-sub'>
						            <a href="{{ (s.nodoArchivo != '#') ? path(s.nodoArchivo) : s.nodoArchivo }}">{{ s.nodoNombre }}</a>
									{% if s.hijos | default() %}
						                <ul>
						                    {{ _self.r(s.hijos) }}
						                </ul>
						            {% endif %}
						        </li>
						    {% endfor %}
						{% endmacro %}
					</ul>
				{% endif %}
			</li>
		{% endfor %}
	</ul>
</div>
{% endspaceless %}


